"custom/weather": {
        "exec": "${HOME}/.config/waybar/weather_script.sh London",
        "return-type": "json",
        "format": "<span foreground=\"#A9B665\">{icon}</span> {} ",
        "format-icons": {
            "?":"?",
            "mm":["", ""],
            "=":["", ""],
            "///":["", ""],
            "//":["", ""],
            "**":["", ""],
            "*/*":["", ""],
            "/":["", ""],
            ".":["", ""],
            "x":["", ""],
            "x/":["", ""],
            "*":["", ""],
            "*/":["", ""],
            "m":["", ""],
            "o":["", ""],
            "/!/":["", ""],
            "!/":["", ""],
            "*!*":["", ""],
            "mmm":["", ""],
            "default":"error",
        },
        "tooltip": true,
        "interval": 900
}
